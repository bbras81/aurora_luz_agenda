generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Cliente {
  id        Int     @id @default(autoincrement())
  nome      String
  morada    String
  cpostal   String
  email     String  @unique
  telemovel String
  quantidade Int
}  